var callCount = 0;
var C = class {
  method(/*{ params }*/) {
    /* implicit strict */
    /*{ body }*/
    callCount++;
  }
};

assert.throws(/*{ error }*/, function() {
  C.prototype.method(/*{ args }*/);
});
assert.sameValue(callCount, 0, 'method body not evaluated');
