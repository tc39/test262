/*---
path: annexB/language/eval-code/direct/func-switch-dflt-
name: >
    Funtion declaration in the `default` clause of a `switch` statement in eval code in the global scope
esid: sec-web-compat-evaldeclarationinstantiation
flags: [noStrict]
---*/

(function(/*{ params }*/) {
  eval(
    '/*{ before }*/switch (1) {' +
    '  default:' +
    '    function f() { /*{ body }*/ }' +
    '}\
    /*{ after }*/'
  );
}(/*{ args }*/));
