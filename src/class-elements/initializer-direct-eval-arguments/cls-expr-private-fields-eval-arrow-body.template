/*---
esid: sec-performeval-rules-in-initializer
path: language/expressions/class/elements/arrow-body-private-direct-
name: direct eval
features: [class, class-fields-private]
---*/

var C = class {
  #x = eval('/*{ arrow-body }*/;');
  x() {
    this.#x();
  }
}

assert.throws(/*{ earlyerror }*/, function() {
  new C().x();
});
