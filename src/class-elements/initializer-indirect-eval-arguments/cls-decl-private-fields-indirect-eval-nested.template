/*---
esid: sec-performeval-rules-in-initializer
path: language/statements/class/elements/nested-private-indirect-
name: indirect eval
features: [class, class-fields-private]
flags: [noStrict]
---*/

var /*{ initializer }*/ = 1;
class C {
  #x = (0, eval)('/*{ initializer }*/;');
  x() {
    return this.#x;
  }
}

assert.sameValue(new C().x(), /*{ initializer }*/);
