/*---
path: language/statements/class/decorator/accessor/with-init/private/instance/
name: private accessor with initializer decorator behavior in class declaration
features: [class, decorators, class-fields-private]
esid: prod-FieldDefinition
---*/

/*{ decorators }*/

class C {
  /*{ application }*/
  accessor #element = /*{ initializer }*/;

  getElement() {
    return this.#element;
  }

  setElement(value) {
    this.#element = value;
  }
}

var classOrInstance = new C();

/*{ assertions }*/
