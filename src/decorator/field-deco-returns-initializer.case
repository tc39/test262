/*---
esid: sec-initializefieldoraccessor
desc: Decorator and initializer order
template: fields/standard/**/*
features: [decorators]
---*/

//- decorators
function dec(value) {
  return (init) => {
    assert.sameValue(init, undefined);
    return 123;
  }
}

//- application
@dec

//- assertions
assert.sameValue(classOrInstance.getElement(), 123);
