/*---
path: language/statements/class/decorator/field/private/static/
name: private static field decorator behavior in class declaration
features: [class, decorators, class-static-fields-private]
esid: prod-FieldDefinition
---*/

/*{ decorators }*/

class C {
  /*{ application }*/
  static #element;

  static getElement() {
    return this.#element;
  }

  static setElement(value) {
    this.#element = value;
  }
}

var classOrInstance = C;

/*{ assertions }*/
