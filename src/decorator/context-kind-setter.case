/*---
esid: sec-createdecoratorcontextobject
desc: Context kind is the string "setter" when decorating a method
template: setters/standard/**/*
features: [decorators]
---*/

//- decorators
function dec(_, context) {
  assert.sameValue(context.kind, "setter");
}

//- application
@dec
