/*---
esid: sec-initializefieldoraccessor
desc: Decorator can return undefined
template: fields/standard/**/*
features: [decorators]
---*/

//- decorators
function dec(value) {
  assert.sameValue(value, undefined);
}

//- application
@dec

//- assertions
assert.sameValue(classOrInstance.getElement(), undefined);
