/*---
esid: sec-createdecoratorcontextobject
desc: Context kind is the string "method" when decorating a method
templates:
  - methods/standard/**/*
  - methods/async/**/*
  - methods/generator/**/*
features: [decorators]
---*/

//- decorators
function dec(_, context) {
  assert.sameValue(context.kind, "method");
}

//- application
@dec
