/*---
esid: sec-createdecoratorcontextobject
desc: Context kind is the string "field" when decorating a field
template: fields/standard/**/*
features: [decorators]
---*/

//- decorators
function dec(_, context) {
  assert.sameValue(context.kind, "field");
}

//- application
@dec
